{
    "manifest_version": 2,
    "name": "MeowMonitor",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://meow.camera/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "32": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    }
}